<div class="all-log-pictures">
  <div class="post-list" style="list-style: "><% @logs.each do |log| %>
    <div>
      <%# div for photos attached to the log, if none, then show the cache photo %>
        <div class="log-picture">
          <% if log.photos.attached? %>
            <%= cl_image_tag log.photos.first.key, width: 100, height: 100 %>
          <% else %>
            <%= image_tag log.seed_photo_url, width: 100, height: 100 %>
          <% end %>
        </div>
        <%# Basic info on log %>
        <%# <%= log.created_on %>
        <%# <%= log.content %>
        <%# <%= link_to "this log show", log_path(log) %>
    </div>
  <% end %>
