<div id="map-container">
  <%= render "map" %>
  <div id="static"></div>
  <%= link_to "+", new_cache_path, id: "new-cache-link" %>
</div>
<div class="position-relative">
  <div class="position-absolute bottom-12 ms-2 mb-4">
    <% if current_user %>
      <i class="fa-regular fa-square-plus fa-xl" id="plus-link"></i>
    <% else %>
      <a class="btn btn-outline-secondary" href="/users/sign_in">sign in</a>
    <% end %>
  </div>
</div>
