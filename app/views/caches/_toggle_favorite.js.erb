<%= link_to "like", toggle_favorite_cach_path(@cache), remote: true, method: :post %>

var cacheId = '<%= @cache.id%>'

function toggleLikeButton(cacheId, innerHTML) {
  let likeButton = document.querySelector(`.like-icon[data-id='${cacheId}']`)
  likeButton.classList.toggle('red');
}

toggleLikeButton(cacheId);
